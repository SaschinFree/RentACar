<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: #CDD7EC;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="za.nmu.wrrv.rent.addVehicleController">
   <children>
      <Label prefHeight="100.0" text="Rent A Car">
         <padding>
            <Insets left="25.0" top="10.0" />
         </padding>
         <font>
            <Font name="System Italic" size="60.0" />
         </font></Label>
      <Line endX="894.0" stroke="#8d918d" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeWidth="5.0">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin></Line>
      <HBox>
         <children>
            <VBox>
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0">
                     <children>
                        <Label prefWidth="200.0" text="Client Number">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <ChoiceBox fx:id="clientNumber" prefWidth="150.0" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0">
                     <children>
                        <Label prefWidth="200.0" text="Registration Number">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField fx:id="registrationNumber" promptText="ABC123 or CUSTOM" />
                        <ChoiceBox fx:id="plateExtension" prefWidth="50.0" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0">
                     <children>
                        <Label prefWidth="200.0" text="Registration Expiration Date">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <DatePicker fx:id="registrationExpirationDate" promptText="YYYY/MM/DD" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0">
                     <children>
                        <Label prefWidth="200.0" text="Vehicle is Insured">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <CheckBox fx:id="vehicleInsurance" mnemonicParsing="false" />
                     </children>
                  </HBox>
                  <Label text="Availability Period" textFill="#8d918d">
                     <font>
                        <Font name="System Italic" size="16.0" />
                     </font>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0">
                     <children>
                        <Label prefWidth="200.0" text="Start Date">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <DatePicker fx:id="vehicleStartDate" promptText="YYYY/MM/DD" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0">
                     <children>
                        <Label prefWidth="200.0" text="End Date">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <DatePicker fx:id="vehicleEndDate" promptText="YYYY/MM/DD" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox>
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0">
                     <children>
                        <Label prefWidth="200.0" text="Make">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField fx:id="vehicleMake" promptText="Volkswagen or BMW" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0">
                     <children>
                        <Label prefWidth="200.0" text="Model">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField fx:id="vehicleModel" promptText="Polo or 530i" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0">
                     <children>
                        <Label prefWidth="200.0" text="Seats">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <Spinner fx:id="vehicleSeats" editable="true" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0">
                     <children>
                        <Label prefWidth="200.0" text="Colour">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField fx:id="vehicleColour" promptText="Red or Colour,Colour etc" />
                     </children>
                  </HBox>
                  <Label prefHeight="25.0">
                     <VBox.margin>
                        <Insets right="10.0" />
                     </VBox.margin>
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0">
                     <children>
                        <Label prefWidth="200.0" text="Cost Multiplier">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField fx:id="costMultiplier" promptText="0.0" />
                     </children>
                  </HBox>
                  <Label prefHeight="100.0" />
               </children>
               <padding>
                  <Insets left="20.0" />
               </padding>
            </VBox>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </HBox>
      <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
         <children>
            <Button fx:id="cancel" mnemonicParsing="false" onMouseClicked="#buttonClicked" style="-fx-background-color: #3B2F2F;" text="Cancel" textFill="WHITE" />
            <Button fx:id="addVehicle" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onMouseClicked="#buttonClicked" style="-fx-background-color: #3B2F2F;" text="Add Vehicle" textFill="WHITE">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="5.0" right="5.0" />
         </padding>
      </HBox>
   </children>
</VBox>
